# Backend Configuration
MAX_AUDIO_QUEUE_SIZE=50
PYTHONPATH=/app/code

# TTS Engine Configuration
TTS_START_ENGINE=orpheus
TTS_ORPHEUS_MODEL=freds0/orpheus-brspeech-3b-0.1-ft-32bits-GGUF

# LLM Configuration
LLM_START_PROVIDER=ollama
LLM_START_MODEL=hf.co/bartowski/huihui-ai_Mistral-Small-24B-Instruct-2501-abliterated-GGUF:Q4_K_M

# Development Settings
NO_THINK=false
DIRECT_STREAM=false
USE_SSL=false
LANGUAGE=pt

# Frontend Configuration
NODE_ENV=production
NEXT_TELEMETRY_DISABLED=1

# Ports
BACKEND_PORT=8000
FRONTEND_PORT=3000 